<!DOCTYPE html>
<html>
  <div>Graphic here</div>
  <div>Description here</div>
  <div>
    <form action="/" method="POST">
			<div>
				<input
					type="text"
					placeholder="Describe what you want to read..."
					name="query"
					value="<%= originalQuery %>"
				>
			</div>
		  <div>
				<button type="submit">Go</button>
			</div>
			<div><%= message %></div>
			<div>
				Results container
				<% if (dataArr != null) { %>
					<% for (var infoObj of dataArr) { %>
						<span>donzo</span>
						<div><%= infoObj %></div>
					<% } %>
				<% } %>
			</div>
    </form>
  </div>
</html>
